{
  "name": "cp3-document-management-system",
  "version": "1.0.0",
  "description": "A simple document management system api, with a web app which consumes the api",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js --config .eslintrc.json ./server/**/*.js && ./node_modules/eslint/bin/eslint.js --config .eslintrc.json ./client/**/*.js",
    "mocha": "mocha --compilers js:babel-core/register server/tests",
    "start": "babel-node app.js",
    "migrate": "sequelize db:migrate",
    "seed": "./node_modules/.bin/babel-node server/tests/helpers/SeedHelper.js",
    "test": "NODE_ENV=test npm run seed && NODE_ENV=test npm run mocha",
    "develop:server": "NODE_ENV=development ./node_modules/gulp/bin/gulp.js develop_server",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec server/tests",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-aolaniran/cp3-document-management-system.git"
  },
  "keywords": [
    "document",
    "management",
    "system",
    "api"
  ],
  "author": "Azeez Olaniran",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-aolaniran/cp3-document-management-system/issues"
  },
  "homepage": "https://github.com/andela-aolaniran/cp3-document-management-system#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "run-sequence": "^1.2.2"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "express": "^4.14.1",
    "fm-log": "^4.4.3",
    "jsonwebtoken": "^7.2.1",
    "morgan": "^1.7.0",
    "faker": "^3.1.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.0",
    "sequelize-cli": "^2.4.0",
    "supertest": "^3.0.0",
    "chai": "^3.5.0"
  }
}
